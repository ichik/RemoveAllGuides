{
  "name": "removeallguides",
  "version": "1.0.1",
  "description": "Removes all the guides from selected artboards.",
  "author": "ichik <ichikumer@gmail.com>",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "RemoveAllGuides",
    "manifest": "src/manifest.json",
    "main": "RemoveAllGuides.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ichik/removeallguides.git"
  },
  "bugs": {
    "url": "https://github.com/ichik/removeallguides/issues"
  },
  "homepage": "https://github.com/ichik/removeallguides#readme",
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  }
}
